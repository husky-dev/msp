{
  "name": "@husky-dev/msp",
  "version": "1.0.0",
  "main": "dist/index.js",
  "author": "Jaroslav Khorishchenko <jaro@husky-dev.me>",
  "license": "MIT",
  "description": "MSP protocol implementation for Node.js",
  "keywords": [
    "MSP",
    "Multiwii",
    "Multiwii Serial Protocol"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/husky-dev/husky-dev-msp.git"
  },
  "files": [
    "dist",
    "LICENSE",
    "README.md"
  ],
  "scripts": {
    "start": "ts-node ./src/example.ts",
    "dist": "tsc",
    "lint": "yarn lint:types && yarn lint:eslint",
    "lint:types": "tsc --noEmit --skipLibCheck",
    "lint:eslint": "eslint \"src/**\"",
    "format": "prettier --write \"src/**/*.ts\"",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "dependencies": {
    "serialport": "12.0.0"
  },
  "devDependencies": {
    "@types/node": "22.10.10",
    "@typescript-eslint/eslint-plugin": "6.18.1",
    "@typescript-eslint/parser": "6.18.1",
    "eslint": "8.56.0",
    "husky": "8.0.3",
    "lint-staged": "15.4.2",
    "prettier": "3.4.2",
    "ts-node": "10.9.2",
    "typescript": "5.7.3"
  }
}
